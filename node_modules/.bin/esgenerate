#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules/escodegen/bin/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules/escodegen/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/node_modules:/mnt/d/Users/HUAWEI/Desktop/node_modules:/mnt/d/Users/HUAWEI/node_modules:/mnt/d/Users/node_modules:/mnt/d/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules/escodegen/bin/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules/escodegen/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/PUBG_Plugin/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/plugins/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/node_modules:/mnt/d/Users/HUAWEI/Desktop/node_modules:/mnt/d/Users/HUAWEI/node_modules:/mnt/d/Users/node_modules:/mnt/d/node_modules:/mnt/d/Users/HUAWEI/Desktop/Yunzai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../escodegen/bin/esgenerate.js" "$@"
else
  exec node  "$basedir/../escodegen/bin/esgenerate.js" "$@"
fi
